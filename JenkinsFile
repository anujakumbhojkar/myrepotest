pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/anujakumbhojkar/myrepotest.git'

            }
        }
        
        stage('Build') {
            steps {
                echo "Buiding"
            }
        }
        
        stage('Test') {
            steps {
                echo "testing"
            }
        }
        
        stage('Deploy') {
            steps {
                echo"deployed"
            }
        }
    }
    
    post {
        success {
            echo 'Pipeline succeeded!'
        }
        failure {
            echo 'Pipeline failed!'
        }
    }
}
